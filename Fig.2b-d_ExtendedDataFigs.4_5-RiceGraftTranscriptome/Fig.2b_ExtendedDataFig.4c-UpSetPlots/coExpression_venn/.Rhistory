readDataFiles <- function(folder) {
fileList <- list.files(pattern="*.tsv", path=folder, full.names=TRUE)
output <- lapply(fileList, read_tsv)
names(output) <- gsub(pattern=paste(folder, "/", sep=""), replacement="", fileList)
return(output)
}
